interf Test {
	func f(x: int) int;

	func g(x: float) float
		=> this.f(5) * x;
}

struct Point {
	a: int;
	b: float;

	constructor(a: int) {
		this.a = a;
		this.b = 5.6;
	}
}

interf for Point is Test {
	func f(x: int) int => this.a + x;
}

func main() {
	let p = new Point(45);
	let t = p.f(14);

	let t2 = t - p.g(1.05);
}

