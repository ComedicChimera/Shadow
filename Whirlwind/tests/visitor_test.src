interf Parent {
	func test() int;

	func test2(x: int) str => "testtest"[this.test():this.test() + x];
}

interf Cool {
	func test3() {}
}

struct Base {
	a: int;
}

interf for Base is Parent, Cool {
	func test() int => this.a * 2;

	func test2(x: int) str {
		return super(Parent).test2(3) + "lol";
	}
}

func main() {

}