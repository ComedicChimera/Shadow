struct Range {
	mul: int;
}

interf for Range {
	operator [](i: int) int
		=> i * this.mul;

	operator [](i: int, v: int) {
		this.mul += v;

		return i * this.mul;
	}
}

func main() {
	let arr = {4, 3, -1, 7};

	arr[2] = 1;

	let x = arr[0];

	let r = new Range();

	r[2] = 4;

	x = r[1];

	let arr2 = {new Range()};

	arr2[0][1] = 5;
}