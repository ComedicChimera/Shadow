struct Node {
	val: int;
	left, right: dyn* Node;
}

func insert(root: dyn* Node, val: int) {
	if (root == null)
		root = make Node { val = val, left = null, right = null };

	if (root->val > val)
		insert(root->left, val);
	else
		insert(root->right, val);
}

func makeTree() {
	let arr = [2, 3, 0, 1, 5, 4, 7, 6, 8];

	let root = make Node { val = arr[0], left = null, right = null };

	for (i = 1; i < 9; i++) {
		insert(root, arr[i]);
	}
}