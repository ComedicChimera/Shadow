interf Test {
	template<T>
	func f(x: T) T => x;

	func fmt(x: int) int => x ~^ 2;
}

enum Color {
	GREEN
	RED
	BLUE
	CYAN
	MAGENTA
	YELLOW
	BLACK
}

struct HSL {
	hue: int;
	saturation: float;
	lightness: float;
}

type A : Test {
	let b: int;

	constructor() {
		this.b = 3;
	}

	constructor(b: int) {
		this.b = this.f(b);
	}

	func lol() int 
		=> this.fmt(2) * this.b;
}

func main() {
	let myColor = Color::GREEN;

	let myHSL = new HSL{hue = 10, saturation = 0.5, lightness = 0.87};

	const a = new A();

	let t: int = a.lol();
}

