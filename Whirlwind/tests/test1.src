interf AI {
	func fn1() int;

	func fn2(x: str) str => x + '!';

	priv func fn3() {
		this.fn1();
	}

	priv func fn4() double;
}

type AT : AI {
	func fn1() int => 3;

	priv func fn4() double => 3.14;
}

func main() {
	let a = new AT();

	const b = a.fn1();

	a.fn2("Hello");

	a.fn3(); // should fail
}